- content_for :sidebar do
  .well.sidebar-nav
    %ul.nav.nav-list
      %li.nav-header Entities
      - @entities.each do |e|
        %li
          = e.name
          %ul
            - e.graphs.uniq.each do |g|
              %li= link_to g.title, "#", :data => { :graph_id => g.id } 

#graphs

- if @graph_ids
  :javascript
    $(document).ready(function() {
      var ids = #{@graph_ids.to_json}
      for (var id in ids) {
        graphs.push(ids[id]);
        fetchAndBuildGraphById(ids[id]);
      }
    });
